<p id="notice"><%= notice %></p>
<img src="http://www.mediafire.com/convkey/a985/olfwspu1upcskouzg.jpg?size_id=3" alt="some_text">
<h1>Listado de tipos de articulos</h1>

<table>
  <thead>
    <tr>
      <th><font color="#49676f">Nombre</font></th>
      <th><font color="#49676f">Descripcion</font></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipos.each do |tipo| %>
      <tr>
        <td><%= tipo.tipo_name %></td>
        <td><%= tipo.tipo_des %></td>
        <td><%= link_to 'Show', tipo %></td>
        <td><%= link_to 'Edit', edit_tipo_path(tipo) %></td>
        <td><%= link_to 'Destroy', tipo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Agregar un nuevo tipo de articulo', new_tipo_path %>
