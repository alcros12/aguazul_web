<h1>Listado de Marcas</h1>

<table border="1" align="center" style="width:50%">
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trade_marks.each do |trade_mark| %>
      <tr>
        <td><%= trade_mark.name %></td>
        <td><%= link_to 'Show', trade_mark %></td>
        <td><%= link_to 'Edit', edit_trade_mark_path(trade_mark) %></td>
        <td><%= link_to 'Destroy', trade_mark, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Trade mark', new_trade_mark_path %>
